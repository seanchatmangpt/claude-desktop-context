{
  "name": "nuxt-cdcs-enhanced",
  "version": "1.0.0",
  "description": "Nuxt.js project with CDCS SPR enhancement for 80%+ efficiency gains",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host --port 3000",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "// === CDCS Core Operations ===": "",
    "cdcs:predict": "node scripts/predict-nuxt-needs.js",
    "cdcs:analyze": "node scripts/analyze-nuxt-patterns.js",
    "cdcs:nuxt-core-analyze": "node scripts/nuxt-pattern-analyzer.js",
    "cdcs:optimize": "node scripts/optimize-nuxt-performance.js",
    "cdcs:health": "node scripts/health-check-nuxt.js",
    "cdcs:status": "node scripts/nuxt-spr-status.js",
    "// === SPR Operations ===": "",
    "spr:activate": "node scripts/activate-nuxt-spr.js",
    "spr:generate": "node scripts/generate-nuxt-spr.js",
    "spr:validate": "node scripts/validate-nuxt-spr.js",
    "spr:evolve": "node scripts/evolve-nuxt-spr.js",
    "// === Pattern Management ===": "",
    "patterns:extract": "node scripts/extract-nuxt-patterns.js",
    "patterns:apply": "node scripts/apply-nuxt-patterns.js",
    "patterns:graph": "node scripts/visualize-pattern-graph.js",
    "// === Autonomous Workflows ===": "",
    "auto:focus": "npm run cdcs:analyze && npm run cdcs:predict && npm run spr:activate",
    "auto:optimize": "npm run cdcs:optimize && npm run build && npm run benchmark:performance",
    "auto:improve": "npm run benchmark:nuxt && npm run spr:evolve && npm run spr:validate",
    "auto:deploy": "npm run build && npm run test:e2e && npm run deploy:preview",
    "auto:predict": "npm run cdcs:predict && npm run spr:activate component_architecture",
    "// === Performance & Benchmarking ===": "",
    "benchmark:nuxt": "node scripts/benchmark-nuxt-performance.js",
    "benchmark:bundle": "nuxi analyze",
    "benchmark:lighthouse": "node scripts/lighthouse-audit.js",
    "benchmark:api": "node scripts/benchmark-api-routes.js",
    "// === Quality Assurance ===": "",
    "test": "vitest",
    "test:unit": "vitest run test/unit",
    "test:integration": "vitest run test/integration",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:e2e": "playwright test",
    "test:performance": "npm run benchmark:lighthouse && npm run benchmark:bundle",
    "test:validate-coverage": "vitest run --coverage && node scripts/validate-coverage.js",
    "test:cucumber": "npx cucumber-js features --require-module ts-node/register --require 'test/cucumber/step-definitions/**/*.steps.ts' --format pretty",
    "test:bdd": "npm run test:cucumber",
    "test:features": "npm run test:cucumber -- --tags @smoke",
    "test:feature": "npx cucumber-js features/$1.feature --require-module ts-node/register --require 'test/cucumber/step-definitions/**/*.steps.ts'",
    "test:spr-bdd": "npx cucumber-js features/spr-activation.feature --require-module ts-node/register --require 'test/cucumber/step-definitions/**/*.steps.ts'",
    "test:performance-bdd": "npx cucumber-js features/performance-benchmarking.feature --require-module ts-node/register --require 'test/cucumber/step-definitions/**/*.steps.ts'",
    "test:bdd-vitest": "vitest run test/bdd",
    "test:spr-bdd-vitest": "vitest run test/bdd/spr-activation.bdd.test.ts",
    "test:performance-bdd-vitest": "vitest run test/bdd/performance-benchmarking.bdd.test.ts",
    "typecheck": "nuxt typecheck",
    "lint": "eslint --ext .ts,.js,.vue .",
    "lint:fix": "eslint --ext .ts,.js,.vue . --fix",
    "// === Build Optimization ===": "",
    "analyze:bundle": "nuxi analyze",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:chunks": "node scripts/optimize-chunks.js",
    "optimize:seo": "node scripts/optimize-seo.js",
    "// === Development Enhancement ===": "",
    "dev:optimized": "npm run spr:activate && npm run dev",
    "dev:analyze": "npm run cdcs:analyze && npm run dev",
    "dev:predict": "npm run auto:predict && npm run dev",
    "// === Deployment ===": "",
    "deploy:preview": "vercel --prod=false",
    "deploy:production": "vercel --prod",
    "deploy:analyze": "npm run benchmark:performance && npm run deploy:preview",
    "// === Continuous Improvement ===": "",
    "improve:bundle": "npm run analyze:bundle && npm run optimize:chunks",
    "improve:seo": "npm run benchmark:lighthouse && npm run optimize:seo",
    "improve:performance": "npm run benchmark:nuxt && npm run optimize:images",
    "improve:api": "npm run benchmark:api && npm run patterns:extract",
    "// === Implementation Loop ===": "",
    "loop:development": "node scripts/nuxt-development-loop.js",
    "loop:validation": "npm run test:coverage && npm run test:integration && node scripts/nuxt-validation-loop.js",
    "loop:continuous": "node scripts/nuxt-continuous-improvement.js",
    "loop:test-integrated": "npm run test:validate-coverage && npm run loop:development"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^11.3.0",
    "@cucumber/gherkin": "^32.1.2",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/test-utils": "^3.9.0",
    "@playwright/test": "^1.40.0",
    "@types/node": "^24.0.3",
    "@vitest/ui": "^1.6.1",
    "c8": "^10.1.3",
    "eslint": "^8.56.0",
    "lighthouse": "^11.4.0",
    "nuxt": "^3.9.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0",
    "vitest": "^1.6.1"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.5.0",
    "pinia": "^2.1.0"
  },
  "cdcs": {
    "version": "3.1.0",
    "sprKernels": [
      "nuxt_component_architecture",
      "nuxt_api_patterns",
      "nuxt_performance_optimization",
      "nuxt_modules_ecosystem",
      "nuxt_ssr_hydration",
      "nuxt_autoimports_optimization",
      "nuxt_seo_meta",
      "nuxt_devtools_integration"
    ],
    "tokenEfficiency": "95%",
    "lastOptimization": "nuxt-core-team-patterns",
    "corePatterns": {
      "moduleUsage": "80% coverage",
      "ssrHydration": "Critical pattern detection",
      "autoImports": "Conflict resolution",
      "seoOptimization": "Production-ready patterns",
      "devToolsIntegration": "Enhanced DX"
    },
    "performanceTargets": {
      "lighthouseScore": 95,
      "bundleSize": "< 250KB",
      "buildTime": "< 30s",
      "apiResponseTime": "< 100ms",
      "hydrationMismatches": 0,
      "moduleOptimization": "< 10 modules"
    }
  }
}
